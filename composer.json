{
    "name": "embarknow/manifoldfactory",
    "description": "Factory implementation for generalised, configured class creation",
    "license": "MIT",
    "authors": [
        {
            "name": "John Porter",
            "email": "john@embarknow.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "git",
            "url": "git://github.com/embarknow/common"
        },
        {
            "type": "git",
            "url": "git://github.com/embarknow/manifoldfactory"
        }
    ],
    "require": {
        "php": ">=5.6",
        "rdlowrey/auryn": "^1.1",
        "embarknow/common": "dev-master"
    },
    "autoload": {

            "EmbarkNow\\Journey\\Factories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmbarkNow\\Journey\\Tests\\Factories\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1"
    }
